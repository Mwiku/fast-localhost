{
    "name": "Fast localhost",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Devops extension. Quickly go to localhost, especially ideal for developers",
    "icons": {
        "128": "icons/fast-localhost.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "incognito": "spanning",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "activeTab"]
}
